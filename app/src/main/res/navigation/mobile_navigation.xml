<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="in.example.aisle.fragments.DiscoverFragment"
        android:label="@string/title_discover"
        tools:layout="@layout/fragment_matches" />

    <fragment
        android:id="@+id/navigation_notes"
        android:name="in.example.aisle.fragments.NotesFragment"
        android:label="@string/title_notes"
        tools:layout="@layout/fragment_discover" />

    <fragment
        android:id="@+id/navigation_matches"
        android:name="in.example.aisle.fragments.MatchesFragment"
        android:label="@string/title_matches"
        tools:layout="@layout/fragment_notes" />
    <fragment
        android:id="@+id/navigation_profile"
        android:name="in.example.aisle.fragments.ProfileFragment"
        android:label="@string/title_profile"
        tools:layout="@layout/fragment_profile" />
</navigation>